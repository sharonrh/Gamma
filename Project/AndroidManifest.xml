<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gamma"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gamma"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="view.HalamanUtama"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="view.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="view.SettingActivity"
            android:label="@string/title_activity_setting_fragment"
            android:parentActivityName="view.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="view.MainActivity" />
        </activity>
        <activity
            android:name="view.NotifikasiActivity"
            android:label="@string/title_activity_notifikasi"
            android:parentActivityName="view.SettingActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="view.SettingActivity" />
        </activity>
        <activity
            android:name="view.TemaActivity"
            android:label="@string/title_activity_tema"
            android:parentActivityName="view.SettingActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="view.SettingActivity" />
        </activity>
        <activity
            android:name="view.TentangActivity"
            android:label="@string/title_activity_tentang"
            android:parentActivityName="view.SettingActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="view.SettingActivity" />
        </activity>
        <activity
            android:name="view.EditProfilActivity"
            android:label="@string/title_activity_edit_profil"
            android:parentActivityName="view.ProfilFragment" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="view.ProfilFragment" />
        </activity>
    </application>

</manifest>